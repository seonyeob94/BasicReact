class Animal {
  constructor() {
    this.x = 0;
    this.y = 0;
  }

  move(x, y) {
    this.x += x;
    this.y += y;
  }
}

// 사용 예시
const ani = new Animal();
console.log("x : " + ani.x);
console.log("y : " + ani.y);
ani.move(10, 10);
console.log("x : " + ani.x);
console.log("y : " + ani.y);

<!-- views/forw.ejs -->
<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <title>FORW 화면</title>
</head>
<body>
  <h2><%= a %>의 친구들</h2>
  <ul>
    <% arr.forEach(name => { %>
      <li><%= name %></li>
    <% }) %>
  </ul>

<h2>사원 리스트</h2>
  <table border="1">
    <thead>
		<tr>
			<th>사원id</th>
			<th>이름</th>
			<th>성별</th>
			<th>주소</th>
		</tr>
	</thead>
	<tbody>
		<% mylist.forEach(vo => { %>
		<tr>
			<td><%= vo.e_id %></td>
			<td><%= vo.e_name %></td>
			<td><%= vo.gen %></td>
			<td><%= vo.addr %></td>
		</tr>
  		<% }) %>
	</tbody>
  </table>
</body>
</html>

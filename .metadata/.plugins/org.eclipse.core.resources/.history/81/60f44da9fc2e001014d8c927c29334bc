const Human = require('./ooptest2');

 const hum = new Human();
  console.log(hum.x);      // 0
  console.log(hum.y);      // 0
  console.log(hum.job);    // student

  hum.move(50, 150);
  hum.chibbo("teacher");

  console.log(hum.x);      // 100
  console.log(hum.y);      // 150
  console.log(hum.job);    // programmer
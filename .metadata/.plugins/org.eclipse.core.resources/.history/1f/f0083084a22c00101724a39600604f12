var jumsu = 98;

if(jumsu>=90){
	console.log(jumsu + " : 수");
}
else if(jumsu>=80){
	console.log(jumsu + " : 우");
}
else if(jumsu>=70){
	console.log(jumsu + " : 미");
}
else if(jumsu>=60){
	console.log(jumsu + " :  양");
}
else{
	console.log(jumsu + " :  가");
}


if(jumsu>=90){
	console.log(`점수 ${jumsu}는 수`);
}
else if(jumsu>=80){
	console.log(`점수 ${jumsu}는 우`);
}
else if(jumsu>=70){
	console.log(`점수 ${jumsu}는 미`);
}
else if(jumsu>=60){
	console.log(`점수 ${jumsu}는 양`);
}
else{
	console.log(`점수 ${jumsu}는 가`);
}